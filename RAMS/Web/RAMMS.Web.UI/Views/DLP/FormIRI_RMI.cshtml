    function openFormAModel(pid, title) {
        //debugger
        //var AssetTypeName = '@ViewBag.AssetType';
            $("body").addClass("loading");
            var filterData = new Object();
            filterData.HeaderNo = pid;

            if (pid != 0 && title != "" && title !=null) {
