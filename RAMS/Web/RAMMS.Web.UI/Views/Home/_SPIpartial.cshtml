                                foreach (var item in rMSPPLPData.OrderBy(a => a.SpiMonth).GroupBy(a => a.SpiMonth).Select(a => a.Key))
                                {
                                    if (item.Value != 0)
                                    {
                                        //decimal val = item.SpiCPlan.Value;
                                        decimal pai = rMSPPLPData.Where(a => a.SpiMonth.Value == item.Value).Sum(a => a.SpiSpi.Value);// val > 0 ? item.SpiCActual.Value / val : 0;
                <td>@string.Format(new System.Globalization.CultureInfo("en-US", false), "{0:n}", Convert.ToDecimal(pai))</td>
 }
            else
            {
<td></td>
}
}
}
                        </tr>
                    </tbody>
                </table>
            </div>

        </div>
    </div>
</div>
