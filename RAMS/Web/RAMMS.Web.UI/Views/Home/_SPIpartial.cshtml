@model RAMMS.Web.UI.Models.FormAModel;
@{ Layout = null;
    string monthPlanned = ViewBag.SpplpMPlanned;
    string monthActual = ViewBag.SpplpMActual;
    string cutPlan = ViewBag.SpplpCPlan;
    string cutActual = ViewBag.SpplpCActual;
    string Planned = ViewBag.SpplpPlannedPer;
    string Actual = ViewBag.Actual;
    string MIRIcurrentMonthSPI = "0.00";
    string MIRIcurrentMonthPAI = "0.00";
    string BTNcurrentMonthSPI = "0.00";
    string BTNcurrentMonthPAI = "0.00";
    int? year = 0;
    int i = 1;
    List<RAMMS.DTO.ResponseBO.DLP.RMDlpSpiDTO> rMSPPLPData = new List<RAMMS.DTO.ResponseBO.DLP.RMDlpSpiDTO>();
    if (ViewBag.SPIData != null)
    {
        rMSPPLPData = (List<RAMMS.DTO.ResponseBO.DLP.RMDlpSpiDTO>)ViewBag.SPIData;
        if (rMSPPLPData.Any())
        {
            year = rMSPPLPData.FirstOrDefault().SpiYear;
        }
@if (rMSPPLPData.Any() && rMSPPLPData.Count(a => a.SpiDivCode.ToUpper() == "MIRI") != 0)
{
    int currentMonth = DateTime.Now.Month;
    foreach (var item in rMSPPLPData.Where(a => a.SpiDivCode.ToUpper() == "MIRI"))
    {
        if (item.SpiMonth == currentMonth)
        {
            MIRIcurrentMonthSPI = string.Format(new System.Globalization.CultureInfo("en-US", false), "{0:n}", item.SpiPai);
            MIRIcurrentMonthPAI = string.Format(new System.Globalization.CultureInfo("en-US", false), "{0:n}", item.SpiSpi);
        }
    }
    foreach (var item in rMSPPLPData.Where(a => a.SpiDivCode.ToUpper() == "BTN"))
    {
        if (item.SpiMonth == currentMonth)
        {
            BTNcurrentMonthPAI = string.Format(new System.Globalization.CultureInfo("en-US", false), "{0:n}", item.SpiPai);
            BTNcurrentMonthSPI = string.Format(new System.Globalization.CultureInfo("en-US", false), "{0:n}", item.SpiSpi);
        }
    }
} }
bool isSPIUpdate = false;
if (ViewBag.SPIUpdate != null)
{
isSPIUpdate = true;
} }
